class Student {
    String name;
    int age;

    
    Student(String name) {
        this.name = name;
        this.age = 18; // default age
    }

   
    Student(String name, int age) {
        this.name = name;
        this.age = age;
    }

    void display() {
        System.out.println(name + " is " + age + " years old.");
    }
}